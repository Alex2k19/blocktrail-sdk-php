{
    "name": "alex2k19/blocktrail-sdk",
    "description": "The BlockTrail PHP SDK, for integration of Bitcoin functionality through the BlockTrail API",
    "keywords": [
        "blocktrail",
        "bitcoin",
        "sdk",
        "api",
        "blockchain",
        "payment",
        "bitcoin data",
        "wallet",
        "transaction"
    ],
    "authors": [
        {
            "name": "Ruben de Vries",
            "email": "ruben@blocktrail.com"
        },
        {
            "name": "Oisin Akiboye Conolly",
            "email": "oisin@blocktrail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Blocktrail\\SDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Blocktrail\\SDK\\Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "require": {
        "bitwasp/bitcoin": "v1.0.4",
        "btccom/justencrypt": "dev-master",
        "btccom/cashaddress": "0.0.3",
        "mdanter/ecc": "0.5.2",
        "guzzlehttp/guzzle": "6.*",
        "99designs/http-signatures-guzzlehttp": "2.0.1",
        "rych/hash_pbkdf2-compat": "1.0.1",
        "ramsey/array_column": "1.1.3",
        "dompdf/dompdf": "0.8.5",
        "endroid/qr-code": "1.5.*",
        "blocktrail/cryptojs-aes-php": "0.1.0",
        "spomky-labs/php-aes-gcm": "1.2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "4.3.*",
        "squizlabs/php_codesniffer": "2.*",
        "php-coveralls/php-coveralls": "^1.0"
    },
    "repositories": {
        "btccom/justencrypt": {
            "type": "vcs",
            "url": "https://github.com/Alex2k19/justencrypt-php"
        }
    }
}
